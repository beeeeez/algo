package meh;

public class meh {

	public static void main(String[] args) {
		double jay = jimmy(5.6,2);
		System.out.println(jay);
		double fay=jammy(5.6,2);
		System.out.println(fay);
		
	}
	public static double jimmy(double jim, int n ) {
		if(n==0) {
			return 1;
		}
		return jim * jimmy(jim, n-1);
	}
	
	public static double jammy(double jim, int n) {
		
		for(int i = n;i>1;i--) {
			  jim=jim*jim;
		}
		return jim;
		
	}
}
