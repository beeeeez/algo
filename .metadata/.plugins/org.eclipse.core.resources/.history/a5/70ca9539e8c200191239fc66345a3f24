package meh;

public class meh {

	public static void main(String[] args) {
		long start = System.nanoTime();
		double jay = jimmy(3,3);
		long end = System.nanoTime();
		System.out.println(jay + " --time: "+(end-start));
		start = System.nanoTime();
		double fay=jammy(3,3);
		end = System.nanoTime();
		System.out.println(fay + " --time: "+(end-start));
		
	}
	public static double jimmy(double jim, int n ) {
		if(n==0) {
			return 1;
		}
		return jim * jimmy(jim, n-1);
	}
	
	public static double jammy(double jim, int n) {
		double fuh=0;
		for(int i=n;i>1;i--) {
			if(fuh==0) {
				fuh=jim*jim;
			}
			else fuh=fuh*jim;
		}
		return jim;
		
	}
}
